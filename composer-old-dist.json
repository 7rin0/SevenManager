{
  "name": "luiseverino-com/symfony-sonata",
  "license": "MIT",
  "type": "project",
  "description": "Built on \"Symfony CMF Standard Edition\" architecture and infrastructure",
  "autoload": {
    "psr-0": {
      "": "src/",
      "Application": "app/"
    }
  },
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/sonata-project/SonataDatagridBundle.git",
      "branch": "master"
    },
    {
      "type": "git",
      "url": "https://github.com/sonata-project/SonataPageBundle.git",
      "branch": "master"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=5.3.9",
    "symfony/symfony": "2.7.5 as 2.3.x-dev",
    "symfony/assetic-bundle": "~2.3",
    "symfony/swiftmailer-bundle": "~2.3",
    "symfony/monolog-bundle": "~2.4",
    "symfony/var-dumper": "^2.7",
    "symfony-cmf/symfony-cmf": "1.2.*",
    "symfony-cmf/simple-cms-bundle": "1.2.*",
    "symfony-cmf/create-bundle": "1.2.*",
    "symfony-cmf/search-bundle": "1.1.*",
    "symfony-cmf/routing-auto-bundle": "~1.0",
    "symfony-cmf/tree-browser-bundle": "~1.1",
    "symfony-cmf/menu-bundle": "~1.2",
    "symfony-cmf/routing-bundle": "~1.3",
    "symfony-cmf/routing": "~1.3",
    "symfony-cmf/content-bundle": "1.2.x-dev",
    "symfony-cmf/block-bundle": "1.2.*",
    "symfony-cmf/seo-bundle": "1.1.*",
    "symfony-cmf/core-bundle": "1.2.0",
    "symfony-cmf/media-bundle": "1.2.1",
    "sonata-project/doctrine-phpcr-admin-bundle": "1.2.3",
    "sonata-project/admin-bundle": "2.3.6 as 2.4.x-dev",
    "sonata-project/cache-bundle": "~2.1",
    "sonata-project/cache": "^1.0.2",
    "sonata-project/datagrid-bundle": "~2.2",
    "sonata-project/page-bundle": "2.3.*",
    "sonata-project/doctrine-orm-admin-bundle": "2.3.3 as 2.4.x-dev",
    "sonata-project/easy-extends-bundle": "^2.1",
    "sonata-project/notification-bundle": "2.2.2",
    "sonata-project/formatter-bundle": "^2.3",
    "sonata-project/intl-bundle": "^2.2",
    "sonata-project/user-bundle": "^2.2",
    "sonata-project/block-bundle": "2.2.13",
    "sonata-project/media-bundle": "2.3.2",
    "sonata-project/doctrine-extensions": "~1.0",
    "doctrine/orm": "~2.2,>=2.2.3,<2.5",
    "doctrine/dbal": "<2.5",
    "doctrine/doctrine-bundle": "~1.2",
    "doctrine/data-fixtures": "~1.0",
    "doctrine/phpcr-odm": "1.2.6",
    "doctrine/phpcr-bundle": "1.2.4",
    "doctrine/doctrine-cache-bundle": "~1.0",
    "twig/extensions": "~1.0",
    "jackalope/jackalope-doctrine-dbal": "1.1.*",
    "nelmio/alice": "~1.0",
    "eko/feedbundle": "~1.0",
    "lunetics/locale-bundle": "~2.3",
    "liip/imagine-bundle": "~1.0",
    "wjzijderveld/check-bundles": "~1.1",
    "helios-ag/fm-elfinder-bundle": "~1.4",
    "burgov/key-value-form-bundle": "~1.0",
    "friendsofsymfony/rest-bundle": "~1.1",
    "jms/serializer-bundle": "~0.11|~1.0",
    "nelmio/api-doc-bundle": "~2.4",
    "cocur/slugify": "~1.0",
    "sonata-project/markitup-bundle": "^2.1@dev"
  },
  "require-dev": {
    "liip/functional-test-bundle": "~1.0",
    "sensio/generator-bundle": "~2.3",
    "sensio/distribution-bundle": "~3.0,>=3.0.12",
    "sensio/framework-extra-bundle": "~3.0,>=3.0.2",
    "incenteev/composer-parameter-handler": "~2.0",
    "symfony-cmf/testing": "1.2.*",
    "symfony/phpunit-bridge": "~2.7|~3.0"
  },
  "replace": {
    "michelf/php-markdown": "^1.5"
  },
  "conflict": {
    "sonata-project/classification-bundle": "2.3.x-dev"
  },
  "scripts": {
    "post-root-package-install": [
      "SymfonyStandard\\Composer::hookRootPackageInstall"
    ],
    "post-install-cmd": [
      "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
      "Symfony\\Cmf\\Bundle\\CreateBundle\\Composer\\ScriptHandler::downloadCreateAndCkeditor",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
    ],
    "post-update-cmd": [
      "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
      "Symfony\\Cmf\\Bundle\\CreateBundle\\Composer\\ScriptHandler::downloadCreateAndCkeditor",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
    ],
    "quick-start": "SevenManagerBundle\\Composer\\ComposerSevenManager::quickStart"
  },
  "suggest": {
    "jackalope/jackalope-jackrabbit": "1.* as PHPCR implementation required by PHPCR-ODM",
    "jackalope/jackalope-doctrine-dbal": "1.* as PHPCR implementation required by PHPCR-ODM",
    "symfony-cmf/seo-bundle": "1.1.* for search engine optimization",
    "symfony-cmf/simple-cms-bundle": "1.2.* for simplified handling of route, content and menu",
    "symfony-cmf/create-bundle": "1.2.* for frontend editing",
    "symfony-cmf/search-bundle": "1.1.* for searching",
    "symfony-cmf/tree-browser-bundle": "1.1.* to support tree navigation in admins",
    "symfony-cmf/routing-auto-bundle": "1.0.* to automatically generate routes on certain events",
    "sonata-project/doctrine-phpcr-admin-bundle": "1.1.* for backend admin"
  },
  "config": {
    "bin-dir": "bin"
  },
  "extra": {
    "symfony-app-dir": "app",
    "symfony-web-dir": "web",
    "symfony-assets-install": "relative",
    "incenteev-parameters": {
      "file": "app/config/parameters.yml"
    },
    "branch-alias": {
      "dev-master": "1.3-dev"
    }
  }
}
